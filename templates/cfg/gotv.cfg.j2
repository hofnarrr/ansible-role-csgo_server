tv_allow_camera_man 1        	// Auto director allows spectators to become camera man
tv_allow_static_shots 1        	// Auto director uses fixed level cameras for shots
tv_autorecord 0            	// Automatically records all games as GOTV demos.
tv_chatgroupsize 0        	// Set the default chat group size
tv_chattimelimit 8        	// Limits spectators to chat only every n seconds
tv_debug 0                	// GOTV debug info.
tv_delay 0                	// GOTV broadcast delay in second
tv_delay1 7			// Secondary GOTV broadcast delay in second
tv_delaymapchange 1        	// Delays map change until broadcast is complete
tv_deltacache 2            	// Enable delta entity bit stream cache
tv_dispatchmode 0            	// Dispatch clients to relay proxies: 0=never, 1=if appropriate, 2=always
tv_enable 1                	// Activates GOTV on server.
tv_enable1 1			// Activates 2nd GOTV on server.
tv_password "{{ csgo_server_tv_password }}"
tv_password1 "{{ csgo_server_tv_password1 }}"
tv_relaypassword "{{ csgo_server_tv_relaypassword }}"
tv_maxrate 128000            	// Max GOTV spectator bandwidth rate allowed, 0 == unlimited
tv_name "{{ csgo_server_tv_name }}"    		// GOTV host name
tv_overridemaster 0        	// Overrides the GOTV master root address.
tv_port {{ csgo_server_tv_port }}            	// Host SourceTV port
tv_port1 {{ csgo_server_tv_port1 }}            	// Host SourceTV port
tv_relayvoice 0            	// Relay voice data: 0=off, 1=on
tv_snapshotrate 128        	// Snapshots broadcasted per second
tv_snapshotrate1 128        	// Snapshots broadcasted per second
tv_timeout 60            	// GOTV connection timeout in seconds.
tv_title "{{ csgo_server_tv_title }}"   		// Set title for GOTV spectator U
tv_transmitall 1            	// Transmit all entities (not only director view)
tv_advertise_watchable 1
tv_allow_camera_man_steamid {{ csgo_server_tv_allow_camera_man_steamid }} //OBSERVER

say "> gotv config loaded <"
